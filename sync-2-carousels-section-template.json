 Pagination Element\n\t\t\tconst slideCount \t\t= slides.length;\n\t\t\tfractionPag.textContent = `2 \/ ${slideCount}`; \/\/ Initial Slide Number\n\t\t\t\n\t\t\tconst thumbCarousel \t\t= carouselWrapper.querySelector('.thumbnail-carousel');\t\/\/ Get Thumbnail Carousel Element\n\t\t\tconst thumbCarouselId \t\t= thumbCarousel.getAttribute('data-script-id'); \/\/ Get Thumbnail Carousel ID\n\t\t\tconst thumbCarouselSlides \t= thumbCarousel.querySelectorAll('.swiper-slide'); \/\/ Get Thumbnail Carousel All Swiper Slides\n\t\t\t\n\t\t\tsetTimeout(function(){ \/\/ Wait for Carousel Swiper Instance ready\n\t\t\t\tconst mainSwiperInstance \t= bricksData.swiperInstances[mainCarouselId];\n\t\t\t\tconst thumbSwiperInstance \t= bricksData.swiperInstances[thumbCarouselId];\n\t\t\t\t\t\t\n\t\t\t\t\/****** Thumbnail Carousel Params ******\/\n\t\t\t\tif ( index == 1 ) {\n\t\t\t\t\tthumbSwiperInstance.params.direction = 'vertical';\n\t\t\t\t}\n\t\t\t\tthumbSwiperInstance.params.slidesPerView = 'auto';\n\t\t\t\tthumbSwiperInstance.params.slidesPerGroup = 1;\n\t\t\t\tthumbSwiperInstance.params.slideToClickedSlide = true;\n\t\t\t\tthumbSwiperInstance.params.freeMode = false;\n\t\t\t\tthumbSwiperInstance.params.centeredSlidesBounds = true;\n\t\t\t\tthumbSwiperInstance.update(); \n\t\t\t\n\t\t\t\t\/\/ Main Carousel slideChange Events\n\t\t\t\tmainSwiperInstance.on('slideChange', function () {\n\t\t\t\t\n\t\t\t\t\t\/\/ Main Carousel Custom Pagination Control\n\t\t\t\t\tfractionPag.textContent = \"\" + (this.realIndex + 1) + \" \/ \" + slideCount + \"\";\n\t\t\t\t\t\n\t\t\t\t\t\/\/ Main Carousel Control Thumbnail Carousel\n\t\t\t\t\tif ( !this.realIndex == 0 || !this.realIndex == 6 ) { \/\/ first and last slide index number\n\t\t\t\t\t\tthumbSwiperInstance.slideTo(this.realIndex, 300);\n\t\t\t\t\t}\n\t\t\t\t\tif ( this.realIndex == 0 || this.realIndex == 6 ) { \/\/ \/\/ first and last slide index number\n\t\t\t\t\t\tthumbCarouselSlides.forEach( function( thumbCarouselSlide ) {\n\t\t\t\t\t\t\tthumbCarouselSlide.classList.remove('swiper-slide-active');\n\t\t\t\t\t\t});\n\t\t\t\t\t\tthumbCarouselSlides[this.realIndex].classList.add('swiper-slide-active');\n\t\t\t\t\t}\n\t\t\t\t\tif ( this.realIndex == 0 ) {\n\t\t\t\t\t\tthumbCarouselSlides[0].parentElement.style.transform = \"translate3d(0px, 0px, 0px)\";\n\t\t\t\t\t}\n\t\t\t\t\t\n\t\t\t\t});\t\n\t\t\t\n\t\t\t\t\/\/ thumb Carousel click Events and Control Main Carousel\n\t\t\t\tthumbSwiperInstance.on('click', function() {\n\t\t\t\t\tmainSwiperInstance.slideTo(this.clickedIndex,300,false); \/\/ 300 is transition time\n\t\t\t\t});\n\t\t\t\t\n\t\t\t\t\/\/ thumb Carousel Drag Events and Control Main Carousel\n\t\t\t\tthumbSwiperInstance.on('slideChange', function() {\n\t\t\t\t\tmainSwiperInstance.slideTo(this.realIndex,300,false); \/\/ 300 is transition time\n\t\t\t\t});\n\t\t\t\t\t\n\t\t\t}, 500); \/\/ You may increase this value if error \"cannot read properties of undefined (reading 'on')\"\n\n\t\t});\n\t});\n<\/script>","executeCode":true,"noRoot":true,"_padding":{"top":"10","bottom":"10"}}},{"id":"qngayy","name":"carousel","parent":"uobrnt","children":[],"settings":{"fields":[{"dynamicData":"{post_title:link}","tag":"h3","dynamicMargin":{"top":20,"right":0,"bottom":20,"left":0},"id":"dda485"},{"dynamicData":"{post_excerpt:20}","id":"ebb0f1"}],"items":{"images":[{"id":438,"full":"https:\/\/anything.local\/wp-content\/uploads\/drone-1-scaled.jpg","url":"https:\/\/anything.local\/wp-content\/uploads\/drone-1-1920x1269.jpg"},{"id":439,"full":"https:\/\/anything.local\/wp-content\/uploads\/drone-2-scaled.jpg","url":"https:\/\/anything.local\/wp-content\/uploads\/drone-2-1920x1442.jpg"},{"id":440,"full":"https:\/\/anything.local\/wp-content\/uploads\/drone-3-scaled.jpg","url":"https:\/\/anything.local\/wp-content\/uploads\/drone-3-1920x1442.jpg"},{"id":471,"full":"https:\/\/anything.local\/wp-content\/uploads\/drone-4-scaled.jpg","url":"https:\/\/anything.local\/wp-content\/uploads\/drone-4-1920x1280.jpg"},{